@model MovieMvcDate.Models.Dto.RequestModel.CreateBookingRequestmodel



  <div class="row">
        <div class="col-md-12">
            <div class="option">
                <h2> Crete Booking</h2>
            </div>
            <form asp-action="CreateBooking">
                <div class="form-group">
                    @* <label for="Id">MovieName </label> *@
                    <input for="Id" type="text" name="Id" id="Id" class="form-control" hidden />
                </div>
                <div class="form-group">
                    <label for="MovieName">MovieName </label>
                    <input for="MovieName" type="text" name="MovieName" id="FirstName" class="form-control" />
                </div>
                 <div class="form-group">
                    <label for="Price">Price </label>
                    <input type="number" name="Price" id="Price" class="form-control"/>
                </div>
                 <div class="form-group">
                    @* <label for="SitNumber" class="control-label">SitNumber</label> *@
                    <input type="number" name="SitNumber" id="SitNumber" class="form-control" readonly hidden/>
                </div>
                      
                    <div class="form-group ">
                        <input href="#myModal" type="submit" value="Submit" class="btn btn-success trigger-btn col-sm-12" data-toggle="modal" />
                    </div>
            </form>
        </div>
    </div>
